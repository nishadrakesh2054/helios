{
    "buildCommand": "npm run build",
    "outputDirectory": "build",
    "headers": [
        {
            "source": "/images/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/((?!static|images|favicon.ico|manifest.json|logo.*\\.svg|.*\\.svg|.*\\.png|.*\\.jpg|.*\\.jpeg|.*\\.gif|.*\\.webp).*)",
            "destination": "/index.html"
        }
    ]
}